<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>Exampaper</servlet-name>
        <servlet-class>Examination.Exampaper</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExamServlet</servlet-name>
        <servlet-class>Examination.ExamServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExampaperServlet</servlet-name>
        <servlet-class>Examination.ExampaperServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Exampaper</servlet-name>
        <url-pattern>/Exampaper</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExamServlet</servlet-name>
        <url-pattern>/ExamServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExampaperServlet</servlet-name>
        <url-pattern>/ExampaperServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
